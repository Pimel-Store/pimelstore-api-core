{
  "version": 2,
  "builds": [
    { "src": "api/*.ts", "use": "@vercel/node" },
    { "src": "utils/*.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "api/home.ts" },
    { "src": "/register", "dest": "api/registerUser.ts", "methods": ["POST"] },
    { "src": "/login", "dest": "api/login.ts", "methods": ["POST"] }
  ]
}
